.gitattributes text eol=lf

*.psd filter=lfs diff=lfs merge=lfs -text
*.wav filter=lfs diff=lfs merge=lfs -text
*.fbx filter=lfs diff=lfs merge=lfs -text
# Unity text assets
*.cs        text eol=lf
*.shader    text eol=lf
*.txt       text eol=lf
*.json      text eol=lf
*.unity     text eol=lf
*.prefab    text eol=lf
*.mat       text eol=lf
*.asmdef    text eol=lf
*.asset     text eol=lf
